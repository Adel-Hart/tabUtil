{
    "name": "tabUtility",
    "version": "0.0.0",
    "description": "save tab and sharing tab",
    "manifest_version":3,

    "action": {
        "default_popup": "popup.html"
    },

    "background": {
        "service_worker":"scripts/background.js"
    },

    "permissions" : [
        "tabs",
        "activeTab",
        "history",
        "downloads"
    ]

}